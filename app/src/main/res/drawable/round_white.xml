<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:left="@dimen/dp_1"
        android:top="@dimen/dp_1"
        android:right="@dimen/dp_1"
        android:bottom="@dimen/dp_1">
        <shape android:shape="oval">
            <solid android:color="@color/picture_color_f2"/>
            <size android:width="@dimen/dp_30"
                android:height="@dimen/dp_30"/>
            <corners android:radius="@dimen/dp_100"/>
